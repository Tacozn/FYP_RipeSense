{% extends "base.html" %}

{% block title %}Analysis Results{% endblock %}

{% block content %}
<div class="text-center mb-5">
    <h2 class="fw-bold">Analysis Results</h2>
</div>
<div class="row align-items-center g-4">
    <div class="col-md-6 text-center">
        <img src="data:image/jpeg;base64,{{ img_str }}" alt="Uploaded Image" class="result-image">
    </div>
    <div class="col-md-6">
        <div class="result-details text-start">
            <h4 class="mb-4 text-center">Detection Details:</h4>

            {% if not_fruit %}
                <div class="alert alert-warning text-center fs-5">üö´ Not a fruit</div>
            {% endif %}

            {% if class_names %}
                <ul class="list-group list-group-flush fs-5">
                    {% for name in class_names %}
                        <li class="list-group-item"><strong>Detected Class:</strong> <span class="badge bg-primary rounded-pill fs-6">{{ name | title }}</span></li>
                    {% endfor %}
                </ul>
            {% endif %}

            {% if ripeness_result %}
                <div class="ripeness-card text-center text-white p-3 rounded mb-3 
                    {% if ripeness_result.ripeness_level == 'Unripe' %} bg-secondary
                    {% elif ripeness_result.ripeness_level == 'Ripe' %} bg-success
                    {% elif ripeness_result.ripeness_level == 'Overripe' %} bg-warning text-dark
                    {% endif %}">
                    <h5 class="mb-0">Ripeness Level</h5>
                    <div class="fs-2 fw-bold">
                        {{ ripeness_result.ripeness_level }}
                    </div>
                    <div class="confidence">
                        Confidence: {{ (ripeness_result.confidence * 100)|round(1) }}%
                    </div>
                </div>

                <div class="color-analysis card p-4">
                    <h5 class="text-center mb-4">Color Analysis</h5>
                    
                    <div class="mb-3">
                        <div class="d-flex justify-content-between align-items-center mb-1">
                            <span class="fw-bold text-success">üü¢ Green (Unripe)</span>
                            <span class="badge bg-success rounded-pill">{{ ripeness_result.color_analysis.green_percentage | default(0) | round }}%</span>
                        </div>
                        <div class="progress" style="height: 20px;">
                            <div class="progress-bar bg-success" role="progressbar" 
                                 data-width="{{ ripeness_result.color_analysis.green_percentage | default(0) | round }}"
                                 aria-valuenow="{{ ripeness_result.color_analysis.green_percentage | default(0) | round }}" 
                                 aria-valuemin="0" aria-valuemax="100">
                            </div>
                        </div>
                    </div>
                    
                    <div class="mb-3">
                        <div class="d-flex justify-content-between align-items-center mb-1">
                            <span class="fw-bold text-warning">üü° Yellow (Ripe)</span>
                            <span class="badge bg-warning text-dark rounded-pill">{{ ripeness_result.color_analysis.yellow_percentage | default(0) | round }}%</span>
                        </div>
                        <div class="progress" style="height: 20px;">
                            <div class="progress-bar bg-warning" role="progressbar" 
                                 data-width="{{ ripeness_result.color_analysis.yellow_percentage | default(0) | round }}"
                                 aria-valuenow="{{ ripeness_result.color_analysis.yellow_percentage | default(0) | round }}" 
                                 aria-valuemin="0" aria-valuemax="100">
                            </div>
                        </div>
                    </div>
                    
                    <div class="mb-3">
                        <div class="d-flex justify-content-between align-items-center mb-1">
                            <span class="fw-bold text-secondary">üü§ Brown (Overripe)</span>
                            <span class="badge bg-secondary rounded-pill">{{ ripeness_result.color_analysis.brown_percentage | default(0) | round }}%</span>
                        </div>
                        <div class="progress" style="height: 20px;">
                            <div class="progress-bar bg-secondary" role="progressbar" 
                                 data-width="{{ ripeness_result.color_analysis.brown_percentage | default(0) | round }}"
                                 aria-valuenow="{{ ripeness_result.color_analysis.brown_percentage | default(0) | round }}" 
                                 aria-valuemin="0" aria-valuemax="100">
                            </div>
                        </div>
                    </div>
                </div>

                <div class="recommendation alert alert-light mt-3">
                    <h5 class="alert-heading">üí° Recommendation</h5>
                    {% if ripeness_result.ripeness_level == 'Unripe' %}
                        <p>This fruit is not yet ripe. Wait a few days at room temperature.</p>
                    {% elif ripeness_result.ripeness_level == 'Ripe' %}
                        <p>Perfect! This fruit is ready to eat for the best taste and texture.</p>
                    {% elif ripeness_result.ripeness_level == 'Overripe' %}
                        <p>This fruit is overripe. Consume it soon or use it for cooking/baking.</p>
                    {% endif %}
                </div>
            {% endif %}
            
            <div class="text-center mt-4">
                <a href="/" class="btn btn-secondary">üè† Run Another Analysis</a>
            </div>
        </div>
    </div>
</div>
{% endblock %}
