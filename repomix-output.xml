from flask import Flask, render_template, request, redirect, url_for, Response, jsonify
from werkzeug.utils import secure_filename
import cv2
from ultralytics import YOLO
import numpy as np
from PIL import Image
import io
import base64

app = Flask(__name__)

ALLOWED_EXTENSIONS = {'png', 'jpg', 'jpeg'}

# Detection filtering to reduce false positives
CONF_THRESHOLD = 0.5  # increase if still seeing false positives
MIN_BOX_REL_AREA = 0.02  # ignore boxes smaller than 2% of frame area

# Relaxed thresholds for single image uploads (to avoid missing small/low-contrast fruit)
UPLOAD_CONF_THRESHOLD = 0.35
UPLOAD_MIN_BOX_REL_AREA = 0.005

# Load YOLO model for fruit detection only
fruit_detection_model = YOLO("weights_3/best.pt")

def allowed_file(filename):
    return '.' in filename and filename.rsplit('.', 1)[1].lower() in ALLOWED_EXTENSIONS

def image_to_base64(image):
    buffered = io.BytesIO()
    image.save(buffered, format="JPEG")
    return base64.b64encode(buffered.getvalue()).decode()

# --- Main Page Routes ---
@app.route('/')
def home():
    return render_template('index.html')

@app.route('/fruit_detection')
def fruit_detection():
    return render_template('fruit_detection.html')

## Disease detection removed

@app.route('/ripeness_detection')
def ripeness_detection():
    return render_template('ripeness_detection.html')

# --- Ripeness Detection Logic ---
@app.route('/detect_ripeness', methods=['GET', 'POST'])
def detect_ripeness():
    if request.method == 'POST':
        file = request.files.get('file')
        if file and allowed_file(file.filename):
            img = Image.open(io.BytesIO(file.read())).convert("RGB")
            # Check if a fruit is detected before running ripeness analysis
            img_bgr = cv2.cvtColor(np.array(img), cv2.COLOR_RGB2BGR)
            results = fruit_detection_model(img_bgr, conf=UPLOAD_CONF_THRESHOLD)
            any_valid_box = False
            if results:
                r0 = results[0]
                if r0.boxes is not None and r0.boxes.xyxy is not None:
                    boxes = r0.boxes.xyxy.cpu().numpy()
                    h, w = img_bgr.shape[:2]
                    frame_area = float(max(1, w * h))
                    confs = r0.boxes.conf.float().cpu().tolist()
                    for (x1, y1, x2, y2), conf in zip(boxes, confs):
                        bw = max(0.0, x2 - x1)
                        bh = max(0.0, y2 - y1)
                        rel_area = float((bw * bh) / frame_area)
                        if conf >= UPLOAD_CONF_THRESHOLD and rel_area >= UPLOAD_MIN_BOX_REL_AREA:
                            any_valid_box = True
                            break

            img_str = image_to_base64(img)
            if not any_valid_box:
                # As a fallback, try ripeness analysis; if it clearly indicates fruit-like colors, show ripeness
                ripeness_result = analyze_fruit_ripeness(img)
                if ripeness_result and ripeness_result.get('ripeness_level') != 'Unknown' and ripeness_result.get('confidence', 0) >= 0.5:
                    return render_template('result_page.html', img_str=img_str, ripeness_result=ripeness_result)
                return render_template('result_page.html', img_str=img_str, not_fruit=True)

            ripeness_result = analyze_fruit_ripeness(img)
            return render_template('result_page.html', img_str=img_str, ripeness_result=ripeness_result)
    return redirect(url_for('ripeness_detection'))

## Disease endpoints removed: banana/mango/pomegranate

# --- Model Inference Functions ---
## Disease inference removed

def analyze_fruit_ripeness(image):
    img_array = np.array(image)
    hsv = cv2.cvtColor(img_array, cv2.COLOR_RGB2HSV)
    
    green_lower = np.array([35, 50, 50])
    green_upper = np.array([85, 255, 255])
    yellow_lower = np.array([20, 50, 50])
    yellow_upper = np.array([35, 255, 255])
    brown_lower = np.array([0, 50, 50])
    brown_upper = np.array([20, 255, 255])
    
    green_mask = cv2.inRange(hsv, green_lower, green_upper)
    yellow_mask = cv2.inRange(hsv, yellow_lower, yellow_upper)
    brown_mask = cv2.inRange(hsv, brown_lower, brown_upper)
    
    green_pixels = cv2.countNonZero(green_mask)
    yellow_pixels = cv2.countNonZero(yellow_mask)
    brown_pixels = cv2.countNonZero(brown_mask)
    
    total_pixels = green_pixels + yellow_pixels + brown_pixels
    
    if total_pixels == 0:
        return {'ripeness_level': 'Unknown', 'confidence': 0.0, 'color_analysis': {'green_percentage': 0,'yellow_percentage': 0,'brown_percentage': 0}}
    
    green_percentage = (green_pixels / total_pixels) * 100
    yellow_percentage = (yellow_pixels / total_pixels) * 100
    brown_percentage = (brown_pixels / total_pixels) * 100
    
    if green_percentage > 50:
        ripeness_level, confidence = 'Unripe', green_percentage / 100
    elif yellow_percentage > 40:
        ripeness_level, confidence = 'Ripe', yellow_percentage / 100
    elif brown_percentage > 30:
        ripeness_level, confidence = 'Overripe', brown_percentage / 100
    else:
        max_percentage = max(green_percentage, yellow_percentage, brown_percentage)
        confidence = max_percentage / 100
        if max_percentage == green_percentage:
            ripeness_level = 'Unripe'
        elif max_percentage == yellow_percentage:
            ripeness_level = 'Ripe'
        else:
            ripeness_level = 'Overripe'
            
    return {
        'ripeness_level': ripeness_level,
        'confidence': round(confidence, 2),
        'color_analysis': {
            'green_percentage': round(green_percentage, 1),
            'yellow_percentage': round(yellow_percentage, 1),
            'brown_percentage': round(brown_percentage, 1)
        }
    }
    
# --- Real-time detection (No changes needed here) ---
@app.route('/video_feed')
def video_feed():
    return Response(generate_frames(), mimetype='multipart/x-mixed-replace; boundary=frame')

@app.route('/detect_objects', methods=['POST'])
def detect_objects():
    image_data = request.json['image_data'].split(',')[1]
    image_bytes = base64.b64decode(image_data)
    image_array = np.frombuffer(image_bytes, dtype=np.uint8)
    image = cv2.imdecode(image_array, cv2.IMREAD_COLOR)
    results = fruit_detection_model(image, conf=CONF_THRESHOLD)
    detected_objects = []
    img_h, img_w = image.shape[:2]
    frame_area = float(img_w * img_h) if img_w and img_h else 1.0
    for result in results:
        if result.boxes is None:
            continue
        boxes = result.boxes.xywh.cpu().numpy()
        clss = result.boxes.cls.cpu().tolist()
        names = result.names
        confs = result.boxes.conf.float().cpu().tolist()
        for (cx, cy, bw, bh), cls, conf in zip(boxes, clss, confs):
            # Filter by size and confidence
            rel_area = float((bw * bh) / frame_area)
            if conf < CONF_THRESHOLD or rel_area < MIN_BOX_REL_AREA:
                continue
            detected_objects.append({'class': names[cls], 'bbox': [float(cx), float(cy), float(bw), float(bh)], 'confidence': float(conf)})
    return jsonify(detected_objects)

def generate_frames():
    camera = cv2.VideoCapture(0)
    while True:
        success, frame = camera.read()
        if not success:
            break
        else:
            fruit_results = fruit_detection_model(frame, conf=CONF_THRESHOLD)
            result = fruit_results[0]
            if result.boxes is not None:
                boxes_xyxy = result.boxes.xyxy.cpu().numpy()
                classes = result.boxes.cls.cpu().tolist()
                confidences = result.boxes.conf.float().cpu().tolist()
            else:
                boxes_xyxy, classes, confidences = [], [], []
            names = result.names if hasattr(result, 'names') else {}

            # Draw green bounding boxes and labels on the original BGR frame
            fh, fw = frame.shape[:2]
            frame_area = float(fw * fh) if fw and fh else 1.0
            for (box, cls, conf) in zip(boxes_xyxy, classes, confidences):
                x1f, y1f, x2f, y2f = box
                bw = max(0.0, x2f - x1f)
                bh = max(0.0, y2f - y1f)
                rel_area = float((bw * bh) / frame_area)
                if conf < CONF_THRESHOLD or rel_area < MIN_BOX_REL_AREA:
                    continue
                x1, y1, x2, y2 = int(x1f), int(y1f), int(x2f), int(y2f)
                color = (0, 255, 0)  # Green in BGR
                thickness = 2
                cv2.rectangle(frame, (x1, y1), (x2, y2), color, thickness)

                label = f"{names.get(cls, str(cls))} {conf:.2f}" if isinstance(names, dict) else f"{int(cls)} {conf:.2f}"

                # Text background box for readability
                (tw, th), baseline = cv2.getTextSize(label, cv2.FONT_HERSHEY_SIMPLEX, 0.5, 1)
                y_text = y1 - 8 if y1 - 8 > th + 2 else y1 + th + 8
                cv2.rectangle(frame, (x1, y_text - th - 4), (x1 + tw + 6, y_text + baseline - 2), color, -1)
                cv2.putText(frame, label, (x1 + 3, y_text), cv2.FONT_HERSHEY_SIMPLEX, 0.5, (0, 0, 0), 1, cv2.LINE_AA)

            ret, buffer = cv2.imencode('.jpg', frame, [cv2.IMWRITE_JPEG_QUALITY, 50])
            frame_bytes = buffer.tobytes()
            yield (b'--frame\r\n'
                   b'Content-Type: image/jpeg\r\n\r\n' + frame_bytes + b'\r\n')

if __name__ == '__main__':
    app.run(host="0.0.0.0", debug=True)
