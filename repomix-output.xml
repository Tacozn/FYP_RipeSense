<file path="templates/fruit_detection.html">
{% extends "base.html" %}

{% block title %}Live Fruit Detection{% endblock %}

{% block content %}
<div class="text-center mb-5">
    <h2 class="fw-bold">Live Fruit Detection</h2>
    <p class="text-muted">Your device's camera will be used to identify fruits in real-time.</p>
</div>
<div class="row g-4">
    <div class="col-lg-7">
        <div class="video-container ratio ratio-1x1 rounded shadow-sm" style="position: relative;">
            <video id="video" autoplay playsinline class="rounded" style="width: 100%; height: 100%;"></video>
            <canvas id="overlay" class="rounded" style="position: absolute; left: 0; top: 0; width: 100%; height: 100%; pointer-events: none;"></canvas>
        </div>
    </div>
    <div class="col-lg-5">
        <div class="card custom-card h-100">
            <div class="card-body p-4">
                <h5 class="card-title text-center mb-3">Detection Info</h5>
                <div id="detected-objects">
                    <p class="text-muted text-center">Point your camera at a fruit...</p>
                </div>
            </div>
        </div>
    </div>
</div>
{% endblock %}

{% block scripts %}
<script>
    // This JavaScript is the same as your original file
    const video = document.getElementById('video');
    const detectedObjectsElement = document.getElementById('detected-objects');
    const overlay = document.getElementById('overlay');
    const overlayCtx = overlay.getContext('2d');

    function initializeCamera() {
        const constraints = { video: { facingMode: 'environment' } };
        navigator.mediaDevices.getUserMedia(constraints)
            .then(stream => {
                video.srcObject = stream;
                syncCanvasSize();
                detectObjects();
            })
            .catch(error => {
                console.error('Error accessing the camera:', error);
                detectedObjectsElement.innerHTML = '<p class="text-danger text-center">Could not access camera. Please grant permission.</p>';
            });
    }

    function detectObjects() {
        setInterval(() => {
            if (video.readyState < 2) return;
            const canvas = document.createElement('canvas');
            canvas.width = video.videoWidth;
            canvas.height = video.videoHeight;
            const ctx = canvas.getContext('2d');
            ctx.drawImage(video, 0, 0, canvas.width, canvas.height);
            const imageData = canvas.toDataURL('image/jpeg');

            fetch('/detect_objects', {
                method: 'POST',
                headers: { 'Content-Type': 'application/json' },
                body: JSON.stringify({ image_data: imageData }),
            })
            .then(response => response.json())
            .then(data => {
                detectedObjectsElement.innerHTML = '';
                drawOverlay(data);
                if (data.length === 0) {
                    detectedObjectsElement.innerHTML = '<p class="text-muted text-center">No fruit detected.</p>';
                } else {
                    data.forEach(object => {
                        const { class: className, confidence } = object;
                        const [rawRipe, fruitName] = className.split(' ');
                        const card = document.createElement('div');
                        card.className = 'alert alert-light';
                        card.innerHTML = `<strong>Fruit:</strong> ${fruitName}<br><strong>Status:</strong> ${rawRipe}<br><strong>Confidence:</strong> ${Math.round(confidence * 100)}%`;
                        detectedObjectsElement.appendChild(card);
                    });
                }
            })
            .catch(error => console.error('Error detecting objects:', error));
        }, 1500);
    }

    function syncCanvasSize() {
        const rect = video.getBoundingClientRect();
        overlay.width = video.videoWidth || rect.width;
        overlay.height = video.videoHeight || rect.height;
    }

    function drawOverlay(detections) {
        syncCanvasSize();
        overlayCtx.clearRect(0, 0, overlay.width, overlay.height);
        if (!detections || detections.length === 0) return;

        // Draw green boxes for each detection. Server sends bbox as [cx, cy, w, h]
        detections.forEach(d => {
            const [cx, cy, w, h] = d.bbox;
            const x1 = cx - w / 2;
            const y1 = cy - h / 2;

            overlayCtx.strokeStyle = '#00FF00';
            overlayCtx.lineWidth = 3;
            overlayCtx.strokeRect(x1, y1, w, h);

            const label = `${d.class} ${(d.confidence * 100).toFixed(0)}%`;
            overlayCtx.font = '14px Poppins, Arial, sans-serif';
            const metrics = overlayCtx.measureText(label);
            const textW = metrics.width + 10;
            const textH = 18;
            overlayCtx.fillStyle = '#00FF00';
            const ty = y1 - 6 < textH ? y1 + textH + 6 : y1 - 6;
            overlayCtx.fillRect(x1, ty - textH, textW, textH);
            overlayCtx.fillStyle = '#000000';
            overlayCtx.fillText(label, x1 + 5, ty - 4);
        });
    }

    initializeCamera();
</script>
{% endblock %}
</file>

